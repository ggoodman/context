{
  "name": "@ggoodman/context",
  "version": "0.0.1",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "description": "A take on ergonomic cancellation and timeout propagation in JavaScript inspired by the Go context package",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "sideEffects": false,
  "scripts": {
    "build": "distilt",
    "dev": "microbundle watch",
    "test": "uvu -r esbuild-register",
    "posttest": "kacl lint",
    "prepack": "npm run build",
    "preversion": "kacl prerelease",
    "version": "kacl release && git add CHANGELOG.md && cp CHANGELOG.md dist/",
    "postversion": "git push --follow-tags && gh-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ggoodman/context.git"
  },
  "keywords": [
    "context",
    "abortsignal",
    "cancellation",
    "go",
    "golang"
  ],
  "author": "Geoff Goodman <ggoodman@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ggoodman/context/issues"
  },
  "homepage": "https://github.com/ggoodman/context#readme",
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  },
  "devDependencies": {
    "@types/node": "^14.14.43",
    "distilt": "^0.10.5",
    "esbuild-register": "^2.5.0",
    "gh-release": "^5.0.0",
    "kacl": "^1.1.1",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4",
    "uvu": "^0.5.1"
  },
  "dependencies": {
    "ts-primitives": "^3.2.0"
  }
}
